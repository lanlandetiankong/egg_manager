<configuration>
	<!-- docs:  https://www.cnblogs.com/gexiaoshan/p/10109882.html -->
   <!-- ID of the service. It should be unique accross the Windows system-->
  <id>EggManager_Service</id>
  <!-- Display name of the service -->
  <name>EggManager_Service</name>
  <!-- Service description -->
  <description>Egg管理系统</description>
   
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
   
  <!--Xmx256m 代表堆内存最大值为256MB -jar后面的是项目名-->
  <arguments>-Xmx256m -jar EggManager.jar</arguments>
  <!--日志模式-->
  <logmode>rotate</logmode>
</configuration>