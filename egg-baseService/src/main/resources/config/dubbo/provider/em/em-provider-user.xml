<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:service protocol="dubbo" ref="roleMenuService"
                   interface="com.egg.manager.api.services.em.user.basic.RoleMenuService"/>
    <dubbo:service protocol="dubbo" ref="rolePermissionService"
                   interface="com.egg.manager.api.services.em.user.basic.RolePermissionService"/>
    <dubbo:service protocol="dubbo" ref="userAccountService"
                   interface="com.egg.manager.api.services.em.user.basic.UserAccountService"/>
    <dubbo:service protocol="dubbo" ref="userGroupService"
                   interface="com.egg.manager.api.services.em.user.basic.UserGroupService"/>
    <dubbo:service protocol="dubbo" ref="userJobService"
                   interface="com.egg.manager.api.services.em.user.basic.UserJobService"/>
    <dubbo:service protocol="dubbo" ref="userRoleService"
                   interface="com.egg.manager.api.services.em.user.basic.UserRoleService"/>
    <dubbo:service protocol="dubbo" ref="userTenantService"
                   interface="com.egg.manager.api.services.em.user.basic.UserTenantService"/>

    <dubbo:service protocol="dubbo" ref="userAccountXlsService"
                   interface="com.egg.manager.api.services.em.user.xls.UserAccountXlsService"/>
</beans>